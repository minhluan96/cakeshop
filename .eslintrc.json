{
    "plugins": ["@typescript-eslint"],
    "extends": [
        "next/core-web-vitals",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "rules": {
        // I suggest you add those two rules:
        "@typescript-eslint/no-unused-vars": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "prefer-const": "off",
        "react/jsx-key": "off"
    }
}
